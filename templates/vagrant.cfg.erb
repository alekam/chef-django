[buildout]
extends = core/buildout.cfg

download-cache = /usr/src/pip/

#offline = true
#install-from-cache=true

[django]
project = main
settings = settings.vagrant
extra-paths =
    ${buildout:directory}/
    ${buildout:directory}/apps
    ${buildout:directory}/compat


[pip]
install =
    --default-timeout=60 -r <%= @pip_requirements_file %>
